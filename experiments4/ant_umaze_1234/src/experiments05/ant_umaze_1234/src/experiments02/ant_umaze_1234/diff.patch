+    _initialized = False
+
+    use_prev = True
+    seed = 100
+    log_dir: str = None
+    run_id: str = None
+    algorithm = 'OLBO'  # possible options: OLBO, baseline, MF
+
+    class pc(BaseFLAGS):
+        bonus_scale = 1
+        lamb = 0.01
+        bonus_stop_time= 30
+
+    class slbo(BaseFLAGS):
+        n_iters = 50
+        n_policy_iters = 10
