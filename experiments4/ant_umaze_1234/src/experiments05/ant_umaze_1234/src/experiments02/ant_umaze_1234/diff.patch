+class HopperEnv(mujoco_env.MujocoEnv, utils.EzPickle, BaseModelBasedEnv):
+
+    def __init__(self, frame_skip=4):
+        self.prev_qpos = None
+        dir_path = os.path.dirname(os.path.dirname(os.path.abspath(__file__)))
