+class GoalRewardBilliard(MazeTask):
+    REWARD_THRESHOLD: float = 0.9
+    PENALTY: float = -0.0001
+    MAZE_SIZE_SCALING: Scaling = Scaling(None, 3.0, None)
+    OBSERVE_BALLS: bool = True
