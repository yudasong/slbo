+        '''
+        theta, thetadot = self.state
+        return np.array([np.cos(theta), np.sin(theta), thetadot])
+        '''
+
+        # for the reward
+        y, x, thetadot = np.cos(th), np.sin(th), thdot
+        u = np.clip(u, -self.max_torque, self.max_torque)[0]
+        costs = y + .1 * np.abs(x) + .1 * (thetadot ** 2) + .001 * (u ** 2)
+        reward = -costs
+
+        g = 10.
